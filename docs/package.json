{
  "name": "acutis-docs",
  "version": "1.0.0",
  "private": true,
  "license": "SEE LICENSE IN license.md",
  "author": "John Jackson",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production concurrently yarn:build:*",
    "build:11ty": "eleventy",
    "build:js": "node scripts/bundle.js",
    "build:postcss": "postcss _assets/style.css -d _site/ --config=scripts",
    "clean": "rm -r _site",
    "deploy": "gh-pages -d _site",
    "predeploy": "yarn clean && yarn build",
    "prestart": "yarn build:js",
    "start": "concurrently yarn:start:*",
    "start:11ty": "eleventy --serve",
    "start:postcss": "postcss _assets/style.css -d _site/ -w --config=scripts"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "concurrently": "^7.0.0",
    "cssnano": "^5.1.3",
    "esbuild": "^0.14.25",
    "fast-glob": "^3.2.11",
    "gh-pages": "^3.2.3",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-table-of-contents": "^0.6.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-custom-properties": "^12.1.4",
    "postcss-import": "^14.0.2",
    "prism-themes": "^1.5.0"
  }
}

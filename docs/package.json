{
  "name": "acutis-docs",
  "version": "1.0.0",
  "private": true,
  "license": "SEE LICENSE IN license.md",
  "author": "John Jackson",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production concurrently yarn:build:*",
    "build:11ty": "ELEVENTY_EXPERIMENTAL=true eleventy",
    "build:js": "node scripts/bundle.js",
    "build:postcss": "postcss _assets/style.css -d _site/",
    "clean": "rm -r _site",
    "deploy": "gh-pages -d _site",
    "predeploy": "yarn clean && yarn build",
    "prestart": "yarn build:js",
    "start": "concurrently yarn:start:*",
    "start:11ty": "ELEVENTY_EXPERIMENTAL=true eleventy --serve",
    "start:postcss": "postcss _assets/style.css -d _site/ -w"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "esbuild": "^0.8.43",
    "fast-glob": "^3.2.5",
    "gh-pages": "^3.1.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "markdown-it-table-of-contents": "^0.5.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.0",
    "prism-themes": "^1.5.0"
  }
}

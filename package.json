{
  "name": "acutis-lang",
  "version": "0.6.0",
  "private": false,
  "description": "A declarative template language for web documents & beyond.",
  "keywords": [
    "rescript",
    "template",
    "templating",
    "template engine"
  ],
  "homepage": "https://johnridesa.bike/acutis/",
  "bugs": {
    "url": "https://github.com/johnridesabike/acutis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnridesabike/acutis.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "John Jackson",
    "url": "https://johnridesa.bike/"
  },
  "exports": {
    ".": {
      "import": "./dist/acutis.mjs",
      "require": "./dist/acutis.js"
    },
    "./lib/*": "./lib/*",
    "./node-utils": "./node-utils.js",
    "./package.json": "./package.json",
    "./src/*": "./src/*"
  },
  "main": "dist/acutis.js",
  "module": "dist/acutis.mjs",
  "bin": {
    "acutis": "cli",
    "acutis-lang": "cli"
  },
  "files": [
    "bsconfig.json",
    "cli",
    "dist/",
    "node-utils.js",
    "src/**/*.res",
    "src/**/*.resi"
  ],
  "scripts": {
    "build": "bsb -make-world",
    "bundle": "rollup --config",
    "clean": "bsb -clean-world",
    "prebundle": "yarn build",
    "prepublishOnly": "yarn bundle && yarn test",
    "preversion": "yarn build && yarn test",
    "start": "bsb -w",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:watch:coverage": "jest --watchAll --coverage"
  },
  "dependencies": {
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "bs-platform": "8.4.2",
    "jest": "^26.6.3",
    "reason-test-framework": "^0.3.2",
    "rollup": "^2.34.2",
    "rollup-plugin-terser": "^7.0.2"
  },
  "engines": {
    "node": ">= 10"
  }
}

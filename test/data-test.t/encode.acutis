{% interface
  encode =
    {
      dict: {a: int, b: string},
      arr: [string],
      arrEmpty: [string],
      some: ?string,
      none: ?string,
      t: false | true,
      f: false | true,
      unknown: _
    }
~%}
{% match encode with {dict, arr, arrEmpty, some, none, t, f, unknown} ~%}
Encoding:
{% Comp dict arr arrEmpty some none t f unknown / %}
Encoding unknowns:
{% UnknownComp dict arr arrEmpty some none t f unknown / %}
{%~ /match ~%}
